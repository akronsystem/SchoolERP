
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/SuperAdmin/Views/Shared/MainMenuLayout.cshtml";
}
<html>
<body>
    <div class="m-content" ng-app="ERP">
        @using System.Configuration;

        <!--Begin::Main Portlet-->
        <div class="m-portlet m-portlet--full-height" ng-controller="EmployeeController">

            <!--begin: Portlet Head-->
            <div class="m-portlet__head">
                <div class="m-portlet__head-caption">
                    <div class="m-portlet__head-title">
                        <h3 class="m-portlet__head-text">
                            Employee Master
                        </h3> 
                    </div>
                </div>
                <div class="m-portlet__head-tools">
                    <ul class="m-portlet__nav">
                        <li class="m-portlet__nav-item">
                            <select ng-model="btnactive" ng-change="Initialize()" class="form-control">

                                <option class="form-control" value="Active" ng-selected="true">Active</option>
                                <option class="form-control" value="Deactive">Deactive</option>
                            </select>
                        </li>
                        <li class="m-portlet__nav-item">
                            <a ng-click="ShowHide()" class="btn btn-outline-info m-btn m-btn--icon">
                                <span>
                                    <i class="la la-plus fa-strong-bold"></i>
                                    <span>New Employee</span>
                                </span>
                            </a>
                        </li>

                    </ul>
                </div>
            </div>


            <div ng-init="Initialize()" class="m-content">

                <div class="m-portlet m-portlet--mobile" ng-show="Visible">

                    <div class="m-portlet__body">
                        <div class="row d-block m-0 mt-1">
                            <div class="table-responsive">
                                <!--begin: Datatable -->
                                <table class="table table-striped- table-bordered table-hover table-checkable table-fixed" id="student-admission-view">
                                    <thead>
                                        <tr>
                                            <th>Name Of The Employee</th>
                                            <th>Mobile Number</th>
                                            <th>Department</th>
                                            <th>Email Id</th>
                                            <th>Action</th>
                                            <th>Status</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="data in EmployeeInfoData">
                                            <td>
                                                {{data.EmployeeName}}
                                            </td>
                                            <td>
                                                {{data.MobileNo }}
                                            </td>
                                            <td>
                                                {{data.Department}}
                                            </td>
                                            <td>
                                                {{data.EmailID}}
                                            </td>


                                            <td>
                                                <div class="action-link">
                                                    @*<a class="print-icon" href="javascript:void(0);" data-toggle="m-tooltip" title="Print" data-html="true"><i class="la la-print"></i></a>
                                                        <a class="view-icon" href="javascript:void(0);" data-toggle="m-tooltip" title="View"><i class="la la-eye"></i></a>*@
                                                    <a class="edit-icon" ng-click="Verify(data.EmployeeID,data.EmployeeCode)" data-toggle="m-tooltip" title="Edit" data-wizard-action="first"><i class="la la-edit"></i></a>
                                                    @*<a class="delete-icon" ng-click="Delete(data.SchoolId)" data-toggle="m-tooltip" title="Deactive"><i class="la la-trash-o"></i></a>*@

                                                </div>
                                            </td>
                                            <td>
                                                <span class="m-switch m-switch--icon m-switch--danger">
                                                    <label>

                                                        <input type="checkbox" ng-checked="{{data.Status}}" ng-click="Delete(data.EmployeeID)">
                                                        <span style="height: 33px;width: 45px;"></span>
                                                    </label>
                                                </span>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>



                            </div>
                        </div>
                    </div>
                </div>

            </div>


            <!--end: Portlet Head-->
            <!--begin: Portlet Body-->
            <div class="m-portlet__body m-portlet__body--no-padding">
                <!--begin: Form Wizard-->
                <div class="m-wizard m-wizard--3 m-wizard--success" id="m_wizard" ng-show="IsVisible">
                    <!--begin: Message container -->
                    <div class="m-portlet__padding-x">

                        <!-- Here you can put a message or alert -->
                    </div>

                    <!--end: Message container -->
                    <div class="row m-row--no-padding">
                        <div class="col-xl-2 col-lg-12">

                            <!--begin: Form Wizard Head -->
                            <div class="m-wizard__head">
                                <!--begin: Form Wizard Progress -->
                                <div class="m-wizard__progress">
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                </div>

                                <!--end: Form Wizard Progress -->
                                <!--begin: Form Wizard Nav -->
                                <div class="m-wizard__nav">
                                    <div class="m-wizard__steps">
                                        <div class="m-wizard__step m-wizard__step--current" m-wizard-target="m_wizard_form_step_1">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>1</span>
                                                </a><div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Personal Details
                                                </div>
                                            </div>
                                        </div>
                                        <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_2">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>2</span>
                                                </a>
                                                <div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Educational Details
                                                </div>
                                            </div>
                                        </div>
                                        <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_3">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>3</span>
                                                </a>
                                                <div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Current Appointment
                                                </div>
                                            </div>
                                        </div>
                                        <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_4">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>4</span>
                                                </a>
                                                <div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Bank Details
                                                </div>
                                            </div>
                                        </div>
                                        <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_5">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>5</span>
                                                </a>
                                                <div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Experience Details
                                                </div>
                                            </div>
                                        </div>
                                        <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_6">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>6</span>
                                                </a>
                                                <div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Document Upload
                                                </div>
                                            </div>
                                        </div>
                                        <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_7">
                                            <div class="m-wizard__step-info">
                                                <a href="#" class="m-wizard__step-number">
                                                    <span>7</span>
                                                </a>
                                                <div class="m-wizard__step-line">
                                                    <span></span>
                                                </div>
                                                <div class="m-wizard__step-label">
                                                    Upload Profile Picture
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!--end: Form Wizard Nav -->
                            </div>

                            <!--end: Form Wizard Head -->
                        </div>
                        <div class="col-xl-10 col-lg-12">

                            <!--begin: Form Wizard Form-->
                            <div class="form-body m-wizard__form">

                                <!--
                                    1) Use m-form--label-align-left class to alight the form input lables to the right
                                    2) Use m-form--state class to highlight input control borders on form validation
                                -->
                                <form class="m-form m-form--label-align-left- m-form--state-" name="form" id="m_form">

                                    <!--begin: Form Body -->
                                    <div class="m-portlet__body m-portlet__body--no-padding">

                                        <!--begin: Form Wizard Step 1-->
                                        <div class="m-wizard__form-step m-wizard__form-step--current" id="m_wizard_form_step_1">

                                            <div class="m-form__heading">
                                                <h3 class="m-form__heading-title m--font-success">Personal Details</h3>
                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">Employee Code<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="text" name="code" id="code" class="form-control m-input" placeholder="Employee Code" autocomplete="off" ng-model="EmployeeCode" value="{{EmployeeCode}}" pattern="[0-9]+" required/>
                                                            <input type="hidden" ng-model="EmployeeID" value="{{EmployeeID}}" />

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row" ng-init="GetEmployeeType()">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Employee Type<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{DepartmentID}}" ng-model="DepartmentID" required>
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="d in EmployeeData" value="{{d.DepartmentID}}" ng-selected="d.DepartmentID == DepartmentID">{{d.Department}}</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row" ng-init="GetSection()">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Section<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{SectionID}}" ng-model="SectionID" required>
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="d in SectionData" value="{{d.SectionId}}" ng-selected="d.SectionId == SectionID">{{d.SectionName}}</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row" ng-init="GetShift()">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Shift Type<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{ShiftID}}" ng-model="ShiftID" required>
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="d in ShiftDetails" value="{{d.ShiftID}}" ng-selected="d.ShiftID == ShiftID">{{d.ShiftName}}</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4  col-form-label">Name<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="text" name="name" id="name" class="form-control m-input" placeholder="Full Name" autocomplete="off" ng-model="EmployeeName" value="{{EmployeeName}}" pattern="[a-zA-Z\s]+" title="Employee Name Should be Alphabets" required/>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row" ng-init="GetDesignation()">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Designation<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{DesignationID}}" ng-model="DesignationID" required>
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="d in DesignationData" value="{{d.DesignationID}}" ng-selected="d.DesignationID == DesignationID">{{d.Designation}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Gender<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{Gender}}" ng-model="Gender" required>
                                                                <option value="0">--Select--</option>
                                                                <option>Male</option>
                                                                <option>Female</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Blood Group<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{BloodGroup}}" ng-model="BloodGroup">
                                                                <option value="0">--Select--</option>
                                                                <option>A+</option>
                                                                <option>B+</option>
                                                                <option>A-</option>
                                                                <option>B-</option>
                                                                <option>AB+</option>
                                                                <option>AB-</option>
                                                                <option>O+</option>
                                                                <option>O-</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>


                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4  col-form-label">Email<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="email" name="email" id="email" class="form-control m-input" placeholder="EmailID" autocomplete="off" ng-model="EmailID" value="{{EmailID}}" required/>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">Alternate Email<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="email" name="alernateemail" id="alternateemail" class="form-control m-input" placeholder="Alternate EmailID" autocomplete="off" ng-model="AlternateEmailID" value="{{AlternateEmailID}}"  />

                                                        </div>
                                                    </div>
                                                </div>


                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4  col-form-label">Mobile No<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="text" name="MobileNo" id="MobileNo" class="form-control m-input" placeholder="Mobile Number" autocomplete="off" ng-model="MobileNo" value="{{MobileNo}}"  pattern="[0-9]{10}" title="Only 10 Digit Number" maxlength="10" min="10" required/>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">Alternate Mobile NO<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="text" name="alermobileno" id="alermobileno" class="form-control m-input" placeholder="Alternate Mobile NO" autocomplete="off" ng-model="AlternateMobileNo" value="{{AlternateMobileNo}}" pattern="[0-9]{10}" title="Only 10 Digit Number" maxlength="10" min="10" />

                                                        </div>
                                                    </div>
                                                </div>


                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">Date of Birth<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <div class="input-group date">
                                                                <input type="text" class="form-control m-input" autocomplete="off" placeholder="Selectdate" id="m_datepicker_1" ng-model="DateofBirth" value="{{DateofBirth}}" required/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <hr />
                                            @*<div class="m-form__section">*@
                                            <div class="m-form__heading">
                                                <h3 class="m-form__heading-title m--font-success">
                                                    Permanent Address Details
                                                    <i data-toggle="m-tooltip" data-width="auto" class="m-form__heading-help-icon " title="Some help text goes here"></i>
                                                </h3>
                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group  row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">Address<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <textarea type="text" name="PermanentAddress" id="PermanentAddress" class="form-control m-input" ng-model="PermanentAddress" value="{{PermanentAddress}}" autocomplete="off" pattern="^[a-zA-Z0-9,.!? ]*$" required></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row" ng-init="GetState()">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">State<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{PermanantStateID}}" ng-model="PermanantStateID" ng-change="GetDistrict(PermanantStateID)">
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="s in StateList" value="{{s.StateID}}" ng-selected="s.StateID == PermanantStateID">{{s.State}}</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row" ng-init="GetDistrict()">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">District<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{PermanantDistrictID}}" ng-model="PermanantDistrictID" ng-change="GetTaluka(PermanantStateID,PermanantDistrictID)">
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="d in ParentDistrict" value="{{d.DistrictID}}" ng-selected="d.DistrictID == PermanantDistrictID">{{d.District}}</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group m-form__group row">
                                                        <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">Taluka<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <select class="form-control" value="{{PermanantTalukaID}}" ng-model="PermanantTalukaID">
                                                                <option value="0">--Select--</option>
                                                                <option ng-repeat="d in TalukaList" value="{{d.TalukaID}}" ng-selected="d.TalukaID == PermanantTalukaID">{{d.Taluka}}</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">City/Village<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="text" name="City" id="City" class="form-control m-input" placeholder="City/Village" ng-model="PermanentCountry" value="{{PermanentCountry}}" />

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-6">
                                                    <div class="form-group row">
                                                        <label class="col-xl-4 col-lg-4 col-form-label">Pincode<span class="required-star">*</span></label>
                                                        <div class="col-xl-8 col-lg-8">
                                                            <input type="text" name="schoolname" id="schoolname" class="form-control m-input" placeholder="Permanant Pincode " ng-model="PermanantPincode" value="{{PermanantPincode}}" />

                                                        </div>
                                                    </div>
                                                </div>


                                            </div>

                                            <hr />
                                            <div class="m-form__heading">
                                                <h3 class="m-form__heading-title m--font-success" ng-click="ShowDiv()">
                                                    Correspondance Address Details
                                                    <i data-toggle="m-tooltip" data-width="auto" class="m-form__heading-help-icon " title="Some help text goes here"></i>
                                                </h3>
                                                <label class="m-checkbox">
                                                    Copy Permanent Address
                                                    <input type="checkbox" ng-change="ShowDiv()" ng-model="Selected">
                                                    <span></span>
                                                </label>

                                            </div>
                                            <div ng-show="IsAddress">
                                                <div class="row">
                                                    <div class="col-xl-6">
                                                        <div class="form-group  row">
                                                            <label class="col-xl-4 col-lg-4 col-form-label">Address<span class="required-star">*</span></label>
                                                            <div class="col-xl-8 col-lg-8">
                                                                <textarea type="text" name="CorrespondanceAddress" class="form-control m-input" autocomplete="off" ng-model="CorrespondanceAddress" value="{{CorrespondanceAddress}}"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xl-6">
                                                        <div class="form-group m-form__group row" ng-init="GetCorrState()">
                                                            <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">State<span class="required-star">*</span></label>
                                                            <div class="col-xl-8 col-lg-8">
                                                                <select class="form-control" value="{{CorrespondanceStateID}}" ng-model="CorrespondanceStateID" ng-change="GetCorrDistrict(CorrespondanceStateID)">
                                                                    <option value="0">--Select--</option>
                                                                    <option ng-repeat="s in StateCorrList" value="{{s.StateID}}" ng-selected="s.StateID == CorrespondanceStateID">{{s.State}}</option>

                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row" ng-show="CorrsIDDistrict">
                                                    <div class="col-xl-6">
                                                        <div class="form-group m-form__group row" ng-init="GetDistrict()">
                                                            <label for="example-text-input" class="col-xl-4 col-lg-4 col-form-label">District<span class="required-star">*</span></label>
