
@{
    ViewBag.Title = "Institute";
    Layout = "~/Areas/SuperAdmin/Views/Shared/MainMenuLayout.cshtml";
}

<!DOCTYPE html>
<html lang="en" class="no-js">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Akron | Institute Regisration</title>
    <!--<link rel="shortcut icon" href="../favicon.ico">-->
    <!--begin::Global Theme Styles -->
    <!--begin::Global Theme Styles -->
    <link href="../assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />
    <!--RTL version:<link href="assets/vendors/base/vendors.bundle.rtl.css" rel="stylesheet" type="text/css" />-->
    <link href="../assets/demo/demo12/base/style.bundle.css" rel="stylesheet" type="text/css" />

    <link href="../assets/css/custom.css" rel="stylesheet" type="text/css" />

</head>
@using System.Configuration;
<body class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default">
    <!-- begin:: Page -->
    <!-- BEGIN: Header -->
    <!-- BEGIN: Brand -->
    <!-- END: Brand -->
    <div class="m-stack__item m-stack__item--fluid m-header-head" id="m_header_nav" ng-app="ERP">


        <!-- END: Left Aside -->
        <div class="m-grid__item m-grid__item--fluid m-wrapper" ng-controller="EmployeeController">
            <!-- BEGIN: Subheader -->
            <!-- END: Subheader -->
            <div class="m-content">

                <div class="m-portlet m-portlet--full-height">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-title">
                                <h3 class="m-portlet__head-text">
                                    Institute Regisration
                                </h3>
                            </div>
                        </div>
                        <div class="m-portlet__head-tools">
                            <ul class="m-portlet__nav">
                                <li class="m-portlet__nav-item">
                                    <select ng-model="btnactive" ng-change="Initialize()" class="form-control">

                                        <option class="form-control" value="Active" ng-selected="true">Active</option>
                                        <option class="form-control" value="Deactive">Deactive</option>
                                    </select>
                                </li>
                                <li class="m-portlet__nav-item">
                                    <a ng-click="ShowHide()" class="btn btn-outline-info m-btn m-btn--icon">
                                        <span>
                                            <i class="la la-plus fa-strong-bold"></i>
                                            <span>New Institute</span>
                                        </span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div ng-init="Initialize()" class="m-content">

                        <div class="m-portlet m-portlet--mobile" ng-show="Visible">

                            <div class="m-portlet__body">
                                <div class="row d-block m-0 mt-1">
                                    <div class="table-responsive">
                                        <!--begin: Datatable -->
                                        <table class="table table-striped- table-bordered table-hover table-checkable table-fixed" id="student-admission-view">
                                            <thead>
                                                <tr>
                                                    <th>Name Of The Institute</th>
                                                    <th>Mobile Number</th>
                                                    <th>Email Id</th>
                                                    <th>Action</th>
                                                    <th>Status</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="data in InstituteInfoData">
                                                    <td>
                                                        {{data.InstituteName}}
                                                    </td>
                                                    <td>
                                                        {{data.LandlineNumber }}
                                                    </td>
                                                   
                                                    <td>
                                                        {{data.InstituteEmail}}
                                                    </td>


                                                    <td>
                                                        <div class="action-link">
                                                            @*<a class="print-icon" href="javascript:void(0);" data-toggle="m-tooltip" title="Print" data-html="true"><i class="la la-print"></i></a>
                                                <a class="view-icon" href="javascript:void(0);" data-toggle="m-tooltip" title="View"><i class="la la-eye"></i></a>*@
                                                            <a class="edit-icon" ng-click="Verify(data.InstituteId)" data-toggle="m-tooltip" title="Edit" data-wizard-action="first"><i class="la la-edit"></i></a>
                                                            @*<a class="delete-icon" ng-click="Delete(data.SchoolId)" data-toggle="m-tooltip" title="Deactive"><i class="la la-trash-o"></i></a>*@

                                                        </div>
                                                    </td>
                                                    <td>
                                                        <span class="m-switch m-switch--icon m-switch--danger">
                                                            <label>

                                                                <input type="checkbox" ng-checked="{{data.Status}}" ng-click="Delete(data.InstituteId)">
                                                                <span style="height: 33px;width: 45px;"></span>
                                                            </label>
                                                        </span>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>



                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <!--begin: Form Wizard-->
                    <div class="m-wizard m-wizard--2 m-wizard--success" id="m_wizard" ng-show="IsVisible">

                        <!--begin: Message container -->
                        <div class="m-portlet__padding-x">

                            <!-- Here you can put a message or alert -->
                        </div>

                        <!--end: Message container -->
                        <!--begin: Form Wizard Head -->
                        <div class="m-wizard__head m-portlet__padding-x">

                            <!--begin: Form Wizard Progress -->
                            <div class="m-wizard__progress">
                                <div class="progress">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>

                            <!--end: Form Wizard Progress -->
                            <!--begin: Form Wizard Nav -->
                            <div class="m-wizard__nav">
                                <div class="m-wizard__steps">
                                    <div class="m-wizard__step m-wizard__step--current" m-wizard-target="m_wizard_form_step_1">
                                        <a href="#" class="m-wizard__step-number">
                                            <span><i class="fa  flaticon-placeholder"></i></span>
                                        </a>
                                        <div class="m-wizard__step-info">
                                            <div class="m-wizard__step-title">
                                                1. INSTITUTE DETAILS
                                            </div>

                                        </div>
                                    </div>
                                    <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_2">
                                        <a href="#" class="m-wizard__step-number">
                                            <span><i class="fa  flaticon-layers"></i></span>
                                        </a>
                                        <div class="m-wizard__step-info">
                                            <div class="m-wizard__step-title">
                                                2. INSTITUTE BANK ACCOUNT DETAILS

                                            </div>
                                        </div>
                                    </div>
                                    <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_3">
                                        <a href="#" class="m-wizard__step-number">
                                            <span><i class="fa  flaticon-layers"></i></span>
                                        </a>
                                        <div class="m-wizard__step-info">
                                            <div class="m-wizard__step-title">
                                                3. UPLOAD INSTITUTE LOGO
                                            </div>
                                        </div>
                                    </div>

                                    <div class="m-wizard__step" m-wizard-target="m_wizard_form_step_4">
                                        <a href="#" class="m-wizard__step-number">
                                            <span><i class="fa  flaticon-layers"></i></span>
                                        </a>
                                        <div class="m-wizard__step-info">
                                            <div class="m-wizard__step-title">
                                                4. UPLOAD HEADER FOR REPORTS
                                            </div>
                                        </div>
                                    </div>



                                </div>
                            </div>

                            <!--end: Form Wizard Nav -->
                        </div>

                        <!--end: Form Wizard Head -->
                        <!--begin: Form Wizard Form-->

                        <div class="form-body m-wizard__form" >

                        <div class="m-wizard__form" >


                            <!--
                            1) Use m-form--label-align-left class to alight the form input lables to the right
                            2) Use m-form--state class to highlight input control borders on form validation
                            -->
                            <form class="m-form m-form--label-align-left- m-form--state-" id="m_form">

                                <!--begin: Form Body -->
                                <div class="m-portlet__body student-admission-wrapper">

                                    <!--begin: Form Wizard Step 1-->
                                    <div class="m-wizard__form-step m-wizard__form-step--current" id="m_wizard_form_step_1">
                                        <div class="row">
                                            <div class="col-xl-12">
                                                <div class="m-form__section m-form__section--first">

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row" ng-init="GetSection()">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">SECTION <span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">

                                                                    <select class="form-control" value="{{SectionId}}" ng-model="SectionId" required>                                                                 

                                                                        <option value="0">--Select--</option>
                                                                        <option ng-repeat="d in SectionData" value="{{d.SectionId}}" ng-selected="d.SectionId == SectionId">{{d.SectionName}}</option>

                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">UDISE NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="UdiseNo" class="form-control m-input" placeholder="UdiseNo" ng-model="UdiseNo" value="{{UdiseNo}}" pattern="[0-9]{12}" title="Invalid Format" maxlength="12" min="12" required>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INSTITUTE NAME<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="InstituteName" ng-model="InstituteName" class="form-control m-input" placeholder="Institute Name" value="{{InstituteName}}" pattern="[a-zA-Z\s]+" title="Institute Name Should be Alphabate only" autocomplete="off" required>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">REGISTRATION NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="regno" id="regno" class="form-control m-input" placeholder="Registration No" ng-model="AffilationNo" value="{{AffilationNo}}" pattern="[0-9]+" required>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">ADDRESS 1<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">

                                                                    <textarea class="form-control m-input" id="add1" rows="2" ng-model="Address1" value="{{Address1}}" pattern="^[a-zA-Z0-9,.!? ]*$"  required></textarea>

                                                                   

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">ADDRESS 2<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">

                                                                    <textarea class="form-control m-input" id="add2" rows="2" ng-model="Address2" value="{{Address2}}" pattern="^[a-zA-Z0-9,.!? ]*$" required></textarea>

                                                                    

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row" ng-init="GetState()">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">SECTION <span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">

                                                                    <select class="form-control" value="{{StateId}}" ng-model="StateId" required>                                                                  

                                                                        <option value="0">--Select--</option>
                                                                        <option ng-repeat="s in StateList" value="{{s.StateID}}" ng-selected="s.StateID == StateId">{{s.State}}</option>

                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">CITY<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">

                                                                    <input type="text" name="city" id="city" class="form-control m-input" placeholder="City Name" value="{{CityName}}" ng-model="CityName" pattern="[a-zA-Z\s]+" title="Required letters" required>                                                                   

                                                                </div>
                                                            </div>
                                                        </div>
                                                       
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">PINCODE<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="pincode" id="pincode"  class="form-control m-input" placeholder="Pincode" value="{{Pincode}}" ng-model="Pincode" pattern="[0-9]+" title="Required Only Digit" required>
                                                                    
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">LANDLINE NUMBER<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="Land1" id="Land1" class="form-control m-input" placeholder="" ng-model="LandlineNumber" value="{{LandlineNumber}}" pattern="[0-9]{11}" title="Only 11 Digit Number" maxlength="11" min="11"  required>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">PHONE NUMBER1<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="Land2" id="Land2" class="form-control m-input" placeholder="" value="{{PhoneNumber1}}" ng-model="PhoneNumber1" pattern="[0-9]{10}" title="Only 10 Digit Number" maxlength="10" min="10" required>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">PHONE NUMBER2<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="phoneno2" id="phonno2" class="form-control m-input" placeholder="" value="{{PhoneNumber2}}" ng-model="PhoneNumber2" pattern="[0-9]{10}" title="Only 10 Digit Number" maxlength="10" min="10" >
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">FAX<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="fax" id="fax" class="form-control m-input" placeholder="" value="{{Fax}}" ng-model="Fax" pattern="\+[0-9]{1,3}\([0-9]{3}\)[0-9]{7}" title="Invalid Fax Number">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row" ng-init="GetBoard()">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">Board <span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <select class="form-control" value="{{BoardId}}" ng-model="BoardId" required>
                                                                        <option value="0">--Select--</option>
                                                                        <option ng-repeat="s in BoardData" value="{{s.BoardID}}" ng-selected="s.BoardID == BoardId">{{s.BoardName}}</option>

                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INSTITUTE EMAIL ADDRESS<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="email" name="iemail" id="iemail" class="form-control m-input" placeholder="InstituteEmail" value="{{InstituteEmail}}" ng-model="InstituteEmail" required>
                                                               </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">PRINCIPAL EMAIL ADDRESS<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="email" name="pemail" id="pemail" class="form-control m-input" placeholder="PrinciaplEmail" value="{{PrinciaplEmail}}" ng-model="PrinciaplEmail">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">WEBSITE<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="website" id="website" class="form-control m-input"  value="{{Website}}" ng-model="Website"  placeholder="https://example.com" pattern="https://.*" size="30">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INSTITUTE ESTT. YEAR<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <div class="input-group date">
                                                                        <input type="text" class="form-control m-input" readonly placeholder="Select date" id="m_datepicker_2" value="{{EstablishedYear}}" ng-model="EstablishedYear" />
                                                                        <div class="input-group-append">
                                                                            <span class="input-group-text">
                                                                                <i class="la la-calendar-check-o"></i>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INSTITUTE ORGANIZATION NAME<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="ioname" id="ioname" class="form-control m-input" placeholder="" value="{{InstituteOrganizationName}}" ng-model="InstituteOrganizationName" pattern="[a-zA-Z\s]+" title="Required Letters">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">ORGANIZATION REG. NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="orno" id="orid" class="form-control m-input" placeholder="Organization RegistrationNo" value="{{OrganizationRegistrationNo}}" ng-model="OrganizationRegistrationNo" pattern="[0-9]+" title="Required Digit">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">ORGANIZATION ADDRESS<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <textarea class="form-control m-input" id="oadd" name="oadd" rows="2" value="{{OrganizationAddress}}" ng-model="OrganizationAddress" pattern="^[a-zA-Z0-9,.!? ]*$" title="Invalid"></textarea>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        @*<div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">PROFESSIONAL TAX REG. CERTIFICATION NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="" class="form-control m-input" placeholder="" value="">
                                                                </div>
                                                            </div>
                                                        </div>*@
                                                    </div>

                                                    @*<div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INSTITUTE RECOGNATION NO.(PRI)<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="" class="form-control m-input" placeholder="" value="">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INSTITUTE RECOGNATION NO.(SEC)<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="" class="form-control m-input" placeholder="" value="">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>*@

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">INDEX NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="indexno" id="indexno" class="form-control m-input" placeholder="Index No" value="{{IndexNo}}" ng-model="IndexNo" pattern="[0-9]+">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">PAN NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="panno" id="panno" class="form-control m-input" placeholder="Pan No" value="{{PanNo}}" ng-model="PanNo" pattern="[0-9]+">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-xl-6">
                                                            <div class="form-group row">
                                                                <label class="col-xl-4 col-lg-4 col-form-label">TAN NO<span class="required-star">*</span></label>
                                                                <div class="col-xl-8 col-lg-8">
                                                                    <input type="text" name="tanno" id="tanno" class="form-control m-input" placeholder="Tan No" value="{{TanNo}}" ng-model="TanNo" pattern="[0-9]+">
                                                                </div>
                                                            </div>
                                                        </div>
